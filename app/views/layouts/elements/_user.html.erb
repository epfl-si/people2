<li>
  <% if authenticated? %>
    <%= button_to icon_label('action.sign_out', 'user-check'), session_path, method: :delete, data: { turbo: false }, class: "btn btn-link"
    %>
  <% else %>
    <%= button_to icon_label('action.sign_in', 'user'), new_session_path,
      method: :get, data: { turbo: false }, class: "btn btn-link"
    %>
<% end %>
</li>
