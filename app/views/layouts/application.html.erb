<!doctype html>
<html lang="en">
  <head>
    <%= render "layouts/elements/head" %>
    <%= csrf_meta_tags %>
    <%= csp_meta_tag %>
    <%= yield :head %>
  </head>

  <body>
    <%= render "layouts/elements/header_minimal" %>
    <% console if Rails.env.development? %>

    <div class="main-container">

      <%= yield :breadcrumbs %>

      <div class="container pt-5 pb-5">
        <main id="main" class="content" role="main" >
          <%= yield %>
        </main>      
      </div>

    </div>
    <script>
      svgPath = '<%= belurl "icons/icons.svg" %>';
      featherSvgPath = '<%= belurl "icons/feather-sprite.svg" %>';
    </script>
  </body>
</html>


