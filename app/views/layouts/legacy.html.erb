<!doctype html>
<html lang="en">
  <head>
    <%= render "layouts/legacy/head" %>
    <%= yield :head %>
    <%= render "layouts/legacy/google" if Rails.env.production? %>
  </head>

  <body>
    <%= render "layouts/legacy/header" %>
    <% console if Rails.env.development? %>

    <div class="main-container pt-5 pb-5">

      <main id="main" class="content" role="main" >
        <%= yield %>
      </main>      

      <%= render "layouts/legacy/footer" %>

    </div>
    <script>
      svgPath = '<%= belurl "icons/icons.svg" %>';
      featherSvgPath = '<%= belurl "icons/feather-sprite.svg" %>';
    </script>
  </body>
</html>


