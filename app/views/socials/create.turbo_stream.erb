<%= turbo_stream.append "socials" do %>
  <%= render "editable_social", social: @social, profile: @profile %>
<% end %>

<%= turbo_stream.replace "social_add" do %>
  <%= render 'add', profile: @profile, selector: @tag_selector %>
<% end %>

<%= turbo_stream.replace "flash-messages", partial: "shared/flash" %>