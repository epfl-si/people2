<div class="list-group-item">
	<h5><%= t "action.add_social" %></h5>
	<div class="row">
		<% selector.each do |(label,value)| %>
	    <%= link_to(
	    	icon_text("plus-circle", t(label)),
	    	new_profile_social_path(@profile, tag: value), 
	      class: "btn btn-secondary btn-sm ml-1 mt-1", method: :get,
	      data: { turbo_stream: true, turbo_method: 'get' }
	    ) %>
		<% end %>
	</div>
</div>
