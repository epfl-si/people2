<%= turbo_frame_tag :socials_index do %>
	<%= turbo_frame_tag "socials", src: nil, target: nil, data: {controller: "sortable"} do %>
	  <% @socials.each do |social| %>
      <%= render 'editable_social', social: social %>
	  <% end %>
	<% end %>
	<%= turbo_frame_tag "new_social" do %>
	  <% unless @tag_selector.empty? %>
			<%= render 'add', profile: @profile, selector: @tag_selector %>
		<% end %>
	<% end %>
<% end %>
