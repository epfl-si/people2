<%= turbo_frame_tag :infosciences_index do %>
	<%= turbo_frame_tag "infosciences", src: nil, target: nil, data: {controller: "sortable"} do %>
		<% @infosciences.each do |infoscience| %>
			<%= render 'editable_infoscience', infoscience: infoscience %>
		<% end %>
	<% end %>

	<div class="list-group-item" %>
		<%= turbo_frame_tag "new_infoscience_form" do %>
		  <%= add_record_button(new_profile_infoscience_path(@profile), name: "infoscience") %>
		<% end %>
	</div>
<% end %>
