<h1>Model Boxes</h1>
<table class="table table-sortable" data-tablesaw-sortable>
  <thead>
    <tr>
      <th data-tablesaw-sortable-col>Kind</th>
      <th data-tablesaw-sortable-col>Subkind</th>
      <th data-tablesaw-sortable-col>Section</th>
      <th data-tablesaw-sortable-col>Label</th>
      <th data-tablesaw-sortable-col>Title</th>
      <th>Action</th>
    </tr>
  </thead>
  <tbody>
    <%= render partial: "model_box", collection:  @boxes %>
  </tbody>
</table>
