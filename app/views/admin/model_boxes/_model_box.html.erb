<tr>
  <td>
    <%= icon(model_box.standard ? 'paperclip' : 'circle') %>
    <%= icon(model_box.show_title ? 'eye' : 'eye-off') %>
    <%= icon(model_box.locked_title ? 'lock' : 'edit-3') %>
  </td>
  <td><%= model_box.kind %></td>
  <td><%= model_box.subkind %></td>
  <td><%= model_box.section.label %></td>
  <td><%= model_box.label %></td>
  <td>
      <%= model_box.title_en %>
      <br/>
      <%= model_box.title_fr %>
  </td>
  <td>
    <%= link_to icon("info"), admin_model_box_path(model_box) %>
    |
    <%= link_to icon("edit"), edit_admin_model_box_path(model_box) %>
  </td>
</tr>
