<div class="w-100 pb-5">
  <main id="main" role="main" class="content">
    <div class="container">
      <div class="d-flex flex-wrap justify-content-between align-items-baseline">
        <h1 class="mr-3"><TMPL_VAR NAME=prenomdisplay> <TMPL_VAR NAME=nomdisplay></h1>
        <!-- link to edit profile -->
      </div>

      <div class="row mt-5"><!-- ROW1 -->
        <div class="col-md-12"><!-- ROW1,single -->
          <div class="row"><!-- ROW2 -->
            <div class="col-md-6"><!-- ROW2,L -->
              <div class="row"> <!-- ROW3 -->
                <div class="col-sm-12 col-md-6 mb-3">
                  <%= render 'photo' %>
                </div>
              </div> <!-- ROW3 -->

              <div class="row"> <!-- ROW4 -->
                <div class="col-sm-12">
                  <%= render 'contact' %>
                  <!-- CONTACT -->
                </div>
                <div class="col-sm-12 ">
                  <div style="width: 80%; padding-top: 15px;">
                    <%= render 'research_ids' %>
                  </div>
                </div>
                <!--
                <TMPL_IF NAME=contact>
                  <TMPL_IF NAME=edit_profile>
                    <div class="col-sm-12 my-3">
                      <TMPL_LOOP NAME=contact>
                        <h5><TMPL_VAR NAME=boxlabel></h5>
                        <TMPL_VAR NAME=boxcontent>
                      </TMPL_LOOP>
                    </div>
                  </TMPL_IF>
                </TMPL_IF>
                -->
              </div> <!-- ROW4 -->
            </div> <!-- ROW2,L -->

            <div class="col-md-6"><!-- ROW2,R -->
              <%= render 'accreds' %>
              <%= render 'admin_data' %>
            </div><!-- ROW2,R -->
          </div><!-- ROW2 -->
        </div><!-- ROW1,single -->
      </div><!-- ROW1 -->
    </div><!-- container -->

    <div class="container">
      <div class="position-relative cmt-5 mt30"><!-- position-relative cmt-5 mt30 -->
        <div class="people-contents mt30"><!-- people-contents -->
          <TMPL_IF NAME=edit_profile><!-- if edit profile -->
            <%= render 'menubar' %>
            <%= render 'boxes' %>
            <%= render 'publications' %>
            <%= render 'research' %>
          </TMPL_IF><!-- if edit profile -->

          <TMPL_IF NAME=teachingact><!-- TEACHING ACT -->
            <%= render 'teaching' %>
          </TMPL_IF><!-- TEACHING ACT -->
        </div><!-- people-contents -->
      </div>
    </div><!-- container -->
  </main>
</div>

<%= render 'all_units' %>
